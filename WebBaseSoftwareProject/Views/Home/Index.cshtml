@using WebBaseSoftwareProject

@model User
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/Style/Main.css" />
    <title>Index</title>
</head>
<body>
    <div>

        <h1>Welcome To the ASCI Art Converter!</h1>
