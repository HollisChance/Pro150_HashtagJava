        <div id="SpaceJam">
            <img src="~/Images/SpaceJamlogoAscii.png" alt="MichealJordansBestMovie" />
        </div>
        <div id="Dwight">
            <img src="~/Images/Dwight.png" alt="DwightTheMighty"  />
        </div>
       <div id="cthulu">
           <img src="~/Images/cthulu.png" alt="The Dark One" />
       </div>
       
    </div>
</body>
</html>
